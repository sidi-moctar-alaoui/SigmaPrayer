<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item>
		<shape>
			<gradient
				android:angle="270"
				android:centerColor="#90000000"
				android:endColor="#90000000"
				android:startColor="#90000000">
			</gradient>
			<stroke
				android:width="1dp"
				android:color="#90A0C8F0">
			</stroke>
			<corners android:radius="3dp">
			</corners>
			<padding
				android:bottom="2dp"
				android:left="2dp"
				android:right="2dp"
				android:top="2dp"
				android:color="#90000000">
			</padding>
		</shape>
	</item>

</selector>